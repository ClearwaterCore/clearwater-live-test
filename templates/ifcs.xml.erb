<?xml version="1.0" encoding="UTF-8"?>
<ServiceProfile>
  <InitialFilterCriteria>
    <TriggerPoint>
      <ConditionTypeCNF>0</ConditionTypeCNF>
      <SPT>
        <ConditionNegated>0</ConditionNegated>
        <Group>0</Group>
        <Method><% method ||= "INVITE" %><%= method %></Method>
        <Extension></Extension>
      </SPT>
      <% if defined?(session_case) %>
       <SPT>
        <ConditionNegated>0</ConditionNegated>
        <Group>0</Group>
        <SessionCase><%= session_case %></SessionCase>
        <Extension></Extension>
      </SPT>
      <% end %>
   </TriggerPoint>
    <ApplicationServer>
      <ServerName>sip:<%= server_name %></ServerName>
      <DefaultHandling>0</DefaultHandling>
    </ApplicationServer>
  </InitialFilterCriteria>
</ServiceProfile>
